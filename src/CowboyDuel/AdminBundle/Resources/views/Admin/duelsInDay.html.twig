{% extends 'CowboyDuelAdminBundle::layout.html.twig' %}
{% block title %}Duels In Day{% endblock %}


{% block right %}

<div class="section">
	<div class="box">
		<div class="title">
			Duels In Day
			<span class="hide"></span>
		</div>
		<div class="content">
         <div class="chart-caption"> duelsInDay_users_new</div>
         <table class="chart" style="width : 100%;">
         <thead>
         <tr>
             <th></th>
             <th style="color : #e10000;">unique visitors</th>
             <th style="color : #000000;">pageviews</th>
         </tr>
         </thead>
         <tbody>
		 {% for e in  data.duelsInDay_users_new %}
			 sum:{{ e.sum }} day:{{ e.day}}<br>
         <tr>
             <th>{{ e.day}}</th><td>{{ e.sum }}</td>
         </tr>
		 {% endfor%}
         </tbody>
         </table>
		 <br>

         <div class="chart-caption"> duelsInDay_users_old</div>
         <table class="chart" style="width : 100%;">
         <thead>
         <tr>
             <th></th>
             <th style="color : #e10000;">unique visitors</th>
             <th style="color : #000000;">pageviews</th>
         </tr>
         </thead>
         <tbody>
         {% for e in  data.duelsInDay_users_old %}
         <tr>
             <th>{{ e.day}}</th><td>{{ e.sum }}</td>
         </tr>
		 {% endfor%}
         </tbody>
         </table>
         <br>
		 <br>

         <div class="chart-caption"> duelsInDay_lastDay_10</div>
         <table class="chart" style="width : 100%;">
         <thead>
         <tr>
             <th></th>
             <th style="color : #e10000;">unique visitors</th>
             <th style="color : #000000;">pageviews</th>
         </tr>
         </thead>
         <tbody>
		 {% for e in  data.duelsInDay_lastDay_10 %}
         <tr>
             <th>{{ e.day}}</th><td>{{ e.sum }}</td>
         </tr>
		 {% endfor%}
         </tbody>
         </table>
		 <br>
         <div class="chart-caption"> duelsInDay_region</div>
         <table class="chart" style="width : 100%;">
         	<thead>
         		<tr>
            		<th></th>
             		<th style="color : #e10000;">unique visitors</th>
             		<th style="color : #000000;">pageviews</th>
        	 	</tr>
         	</thead>
         	<tbody>
		 		{% for e in  data.duelsInDay_region %}
         		 <tr>
             		<th>{{ e.day}}</th><td>{{ e.sum }}</td><td>{{ e.region }}</td>
         	 	</tr>
		 		{% endfor%}
         	</tbody>
         </table>
		 <br>


		</div>

	</div>
</div>

{% endblock %}